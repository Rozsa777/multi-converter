{
  "name": "Multi-Converter",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "いろいろなデータの形式変換ツール",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Multi-Converter",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
